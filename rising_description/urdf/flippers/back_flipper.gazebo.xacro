<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:macro name="back_flipper_gazebo" params="prefix">
        <gazebo reference="${prefix}flipper_link">
            <mu1 value="0.0"/>
			<mu2 value="1.0"/>
			<kp value="10000000.0" />
			<kd value="1.0" />
			<fdir1 value="1 0 0"/>
			<material>Gazebo/Grey</material>
            <turnGravityOff>false</turnGravityOff>
        </gazebo>
    </xacro:macro>

</robot>